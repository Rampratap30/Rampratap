public static farthestfromzero(int N, int [] Arr) {
    TreeSet<Integer> ts = new TreeSet<Integer>();
    for (int i=0; i<N; i++){
          ts.add(Arr[i]);
    }
  return ts.last();
}

public static int farthestfromzero(int N, int [] Arr) {
    TreeSet<Integer> ts = new TreeSet<Integer>();
    for (int i=0; i<N; i++){
          ts.add(Arr[i]);
    }
  int maxV = ts.last();
  int minV = ts.first();

  if(Math.abs(minV) >= maxV){
      return minV;
  }
  return maxV;

}

